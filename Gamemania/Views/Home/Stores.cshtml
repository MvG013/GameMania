@using Gamemania.Models;
@model List<WinkelModel>
@{
    ViewBag.Title = "Stores";
}

<div class="jumbotron">

@foreach (WinkelModel Winkels in Model)
{

<p> Plaats: @Winkels.Naam <br />
    Adres: @Winkels.Adres.Straatnaam @Winkels.Adres.Huisnummer @Winkels.Adres.Toevoeging <br />
    Postcode: @Winkels.Adres.Postcode<br />
    Telefoonnummer: @Winkels.Telefoonnummer<br /> </p>
}

</div>